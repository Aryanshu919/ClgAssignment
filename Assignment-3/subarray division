#include<iostream>
using namespace std;
int main(){
    int n,d,m;
    cin >>n;
    int s[n];
    for(int i=0; i<n; i++){
        cin>>s[i];
    }
    cin>>d>>m;
    int temp =0;
    for(int i =0; i<n; i++){
        int sum =0;
         for(int j=i; j<i+m; j++){
             sum = sum +s[j];
    }
    if(sum == d){
        temp++;
        
    }
    }
    cout << temp;
    
   
    return 0;
}
